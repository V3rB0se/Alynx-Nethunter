};

&msm_sdw_codec {
	status = "disabled";
};

&qupv3_se2_spi {
	status = "ok";
	mnh-spi@0 {
		compatible = "realtek,rt5514";
		reg = <0>;
		interrupt-parent = <&tlmm>;
		interrupts = <126 0>;
		irq-gpio = <&tlmm 126 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&rt5514_irq>;
		spi-max-frequency = <10000000>;
		spi-cpol;
		spi-cpha;
		realtek,hotword-ignore-ms = <1000>;
		realtek,musdet-ignore-ms = <2000>;
	};
};

