&pm660_gpios {
	gpio@c100 {
		reg = <0xc100 0x100>;
		qcom,pin-num = <2>;
		qcom,mode = <1>;
		qcom,vin-sel = <0>;
		qcom,src-sel = <2>;
		qcom,pull = <5>;
		qcom,out-strength = <2>;
		qcom,master-en = <1>;
		status = "okay";
	};
};

